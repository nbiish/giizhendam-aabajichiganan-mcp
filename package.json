{
  "name": "@nbiish/giizhendam-aabajichiganan-mcp",
  "version": "0.3.13",
  "description": "MCP Server to interface with aider CLI for specialized tasks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aider-mcp-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && echo '#!/usr/bin/env node\n' | cat - dist/index.js > temp && mv temp dist/index.js && chmod +x dist/index.js",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "aider",
    "llm",
    "ai",
    "tools"
  ],
  "author": "AI Assistant",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@modelcontextprotocol/sdk": "^1.10.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.30",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist"
  ]
}
